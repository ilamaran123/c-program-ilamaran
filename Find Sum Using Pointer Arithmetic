#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    int i;
    
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int *ptr = arr;   // Pointer to array
    long long sum = 0;

    for(i = 0; i < n; i++) {
        sum += *(ptr + i);   // Pointer arithmetic
    }

    printf("%lld", sum);

    return 0;
}
