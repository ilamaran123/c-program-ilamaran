#include <stdio.h>

int main() {
    int n, m, i, j;

    scanf("%d %d", &n, &m);

    int a[n][m];

    // Read matrix elements
    for(i = 0; i < n; i++) {
        for(j = 0; j < m; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // Find and print maximum in each row
    for(i = 0; i < n; i++) {
        int max = a[i][0];   // Assume first element is maximum

        for(j = 1; j < m; j++) {
            if(a[i][j] > max) {
                max = a[i][j];
            }
        }

        printf("%d\n", max);
    }

    return 0;
}
