#include <stdio.h>

int main()
{
    int n, i;
    int arr[1000];
    int flag = 1;

    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    for(i = 0; i < n / 2; i++)
    {
        if(arr[i] != arr[n - 1 - i])
        {
            flag = 0;
            break;
        }
    }

    if(flag == 1)
        printf("YES");
    else
        printf("NO");

    return 0;
}
