
#include <stdio.h>

int main()
{
    int n, i;
    int arr[20];
    int min;

    // Read number of elements
    scanf("%d", &n);

    // Read array elements
    for(i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }

    // Assume first element is minimum
    min = arr[0];

    // Find least element
    for(i = 1; i < n; i++)
    {
        if(arr[i] < min)
        {
            min = arr[i];
        }
    }

    // Print minimum element
    printf("%d", min);

    return 0;
}
