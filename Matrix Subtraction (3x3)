#include <stdio.h>

int main() {
    int a[3][3], b[3][3];
    int i, j;

    // Input first matrix
    for(i=0;i<3;i++)
        for(j=0;j<3;j++)
            scanf("%d",&a[i][j]);

    // Input second matrix
    for(i=0;i<3;i++)
        for(j=0;j<3;j++)
            scanf("%d",&b[i][j]);

    // Subtract and print
    for(i=0;i<3;i++) {
        for(j=0;j<3;j++) {
            printf("%d ", a[i][j] - b[i][j]);
        }
        printf("\n");
    }

    return 0;
}
