#include <stdio.h>

int main()
{
    int n, i;
    int num, sum = 0;

    scanf("%d", &n);

    // Sum of first N natural numbers
    int total = n * (n + 1) / 2;

    // Read N-1 numbers and find their sum
    for(i = 0; i < n - 1; i++)
    {
        scanf("%d", &num);
        sum += num;
    }

    // Missing number
    printf("%d", total - sum);

    return 0;
}
